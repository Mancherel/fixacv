    "faq": {
      "items": {
        "whatIs": {
          "q": "What is fixacv?",
          "a": "fixacv is a free, privacy-first CV/resume builder. Everything runs in your browser and your data stays on your device."
        },
        "account": {
          "q": "Do I need an account?",
          "a": "No. There is no signup or cloud storage. Your CV is saved locally in your browser."
        },
        "exportPdf": {
          "q": "Can I export to PDF?",
          "a": "Yes. The preview matches the PDF, and the export is ready for printing and sending."
        },
        "linkedIn": {
          "q": "Can I import from LinkedIn?",
          "a": "Yes. You can import LinkedIn CSV exports to prefill your profile, experience, education, skills, and courses."
        },
        "languages": {
          "q": "Is it available in English and Swedish?",
          "a": "Yes. The app supports English and Swedish content, with a focus on clean, Scandinavian-style CVs."
